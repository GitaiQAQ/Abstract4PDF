{
    "compileOnSave": false,
    "compilerOptions": {
        "allowJs": false,
        "allowSyntheticDefaultImports": true,
        "lib": [
            "dom",
            "es2015"
        ],
        "jsx": "preserve",
        "target": "es5",
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": false,
        "skipLibCheck": true
    }
}